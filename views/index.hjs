<!DOCTYPE html>
<html>
  <head>
    <title>{{ title }}</title>
    <link href="http://netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="/stylesheets/style.css" />

    <script src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-2.0.3.js" type="text/javascript"></script>
    <script src="http://netdna.bootstrapcdn.com/bootstrap/3.0.0/js/bootstrap.min.js"></script>
    <script src="/javascripts/jquery.mini-editable.js"></script>   
    <script src="/reload/reload.js"></script>
  </head>
  <body>
    <h1 id="main-title">{{ title }}</h1>
    <div class="container">
      <table class="table">
        <caption>An example table</caption>
        <thead>
          <tr>
            <th id="th1">header 1</th>
            <th id="th2">header 2</th>
            <th id="th3">header 3</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td id="d11">data</td>
            <td id="d12">data</td>
            <td id="d13">data</td>
          </tr>
          <tr>
            <td id="d21">data</td>
            <td id="d22">data</td>
            <td id="d23">data</td>
          </tr>
          <tr>
            <td id="d31">data</td>
            <td id="d32">data</td>
            <td id="d33">data</td>
          </tr>
        </tbody>
      </table>
    </div>
    <script>

      $(document).ready(function() {
        $.each($('h1,td,th'), function() {
          $(this).makeEditable(function (event) {
            console.log(event.toString());
          });
        });
      });
    </script>
  </body>
</html>